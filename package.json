        "fs-thumbnail": "^1.0.9",
        "helmet": "^8.1.0",
        "ioredis": "^5.8.0",
        "mime-types": "^3.0.1",
        "mysql2": "^3.15.1",
        "nestjs-paginate": "^12.8.3",
        "nestjs-request-context": "^4.0.0",
        "nodemailer": "^7.0.6",
