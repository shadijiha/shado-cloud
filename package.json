        "@types/express": "^5.0.3",
        "@types/jest": "^30.0.0",
        "@types/multer": "^2.0.0",
        "@types/node": "^24.6.2",
        "@types/passport": "^1.0.17",
        "@types/passport-jwt": "^4.0.1",
        "@types/supertest": "^6.0.3",
        "@types/ws": "^8.18.1",
        "chokidar": "^4.0.3",
        "eslint-config-prettier": "^10.1.8",
        "eslint-config-standard-with-typescript": "^43.0.1",
